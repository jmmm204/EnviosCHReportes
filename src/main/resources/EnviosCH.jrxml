<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="EnviosCH" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">

    <property name="com.jaspersoft.studio.data.defaultdataadapter" value="One Empty Record"/>

    <field name="descripcion" class="java.lang.String"/>
    <field name="peso" class="java.lang.Double"/>
    <field name="tipoEnvio" class="java.lang.Object"/>
    <field name="costoEnvio" class="java.math.BigDecimal"/>
    <field name="almacen" class="java.lang.Object"/>
    <field name="agenteAsignado" class="java.lang.Object"/>

    <background>
        <band splitType="Stretch"/>
    </background>

    <title>
        <band height="60" splitType="Stretch">
            <rectangle>
                <reportElement x="0" y="0" width="555" height="50" backcolor="#E0E0E0"/>
                <graphicElement>
                    <pen lineWidth="0.0"/>
                </graphicElement>
            </rectangle>
            <staticText>
                <reportElement x="10" y="10" width="535" height="30" forecolor="#333333"/>
                <textElement textAlignment="Center" verticalAlignment="Middle">
                    <font size="22" isBold="true"/>
                </textElement>
                <text><![CDATA[REPORTE DETALLADO ENVÍOS CH]]></text>
            </staticText>
        </band>
    </title>

    <columnHeader>
        <band height="30" splitType="Stretch">
            <rectangle>
                <reportElement x="0" y="0" width="555" height="30" backcolor="#006699"/>
            </rectangle>

            <staticText>
                <reportElement x="5" y="0" width="100" height="30" forecolor="#FFFFFF"/>
                <textElement verticalAlignment="Middle"><font size="10" isBold="true"/></textElement>
                <text><![CDATA[Descripción]]></text>
            </staticText>

            <staticText>
                <reportElement x="110" y="0" width="50" height="30" forecolor="#FFFFFF"/>
                <textElement verticalAlignment="Middle"><font size="10" isBold="true"/></textElement>
                <text><![CDATA[Peso (lb)]]></text>
            </staticText>

            <staticText>
                <reportElement x="165" y="0" width="65" height="30" forecolor="#FFFFFF"/>
                <textElement verticalAlignment="Middle"><font size="10" isBold="true"/></textElement>
                <text><![CDATA[Tipo]]></text>
            </staticText>
            <staticText>
                <reportElement x="235" y="0" width="100" height="30" forecolor="#FFFFFF"/>
                <textElement verticalAlignment="Middle"><font size="10" isBold="true"/></textElement>
                <text><![CDATA[Almacén]]></text>
            </staticText>
            <staticText>
                <reportElement x="340" y="0" width="100" height="30" forecolor="#FFFFFF"/>
                <textElement verticalAlignment="Middle"><font size="10" isBold="true"/></textElement>
                <text><![CDATA[Agente]]></text>
            </staticText>
            <staticText>
                <reportElement x="445" y="0" width="110" height="30" forecolor="#FFFFFF"/>
                <textElement verticalAlignment="Middle"><font size="10" isBold="true"/></textElement>
                <text><![CDATA[Costo]]></text>
            </staticText>
        </band>
    </columnHeader>

    <detail>
        <band height="25" splitType="Stretch">
            <textField textAdjust="StretchHeight">
                <reportElement x="5" y="0" width="100" height="20"/>
                <textElement verticalAlignment="Middle"><font size="9"/></textElement>
                <textFieldExpression><![CDATA[$F{descripcion}]]></textFieldExpression>
            </textField>

            <textField>
                <reportElement x="110" y="0" width="50" height="20"/>
                <textElement verticalAlignment="Middle"><font size="9"/></textElement>
                <textFieldExpression><![CDATA[$F{peso} + " lb"]]></textFieldExpression>
            </textField>

            <textField>
                <reportElement x="165" y="0" width="65" height="20"/>
                <textElement verticalAlignment="Middle"><font size="9"/></textElement>
                <textFieldExpression><![CDATA["" + $F{tipoEnvio}]]></textFieldExpression>
            </textField>

            <textField textAdjust="StretchHeight">
                <reportElement x="235" y="0" width="100" height="20"/>
                <textElement verticalAlignment="Middle"><font size="9"/></textElement>
                <textFieldExpression><![CDATA["" + $F{almacen}]]></textFieldExpression>
            </textField>

            <textField textAdjust="StretchHeight">
                <reportElement x="340" y="0" width="100" height="20"/>
                <textElement verticalAlignment="Middle"><font size="9"/></textElement>
                <textFieldExpression><![CDATA["" + $F{agenteAsignado}]]></textFieldExpression>
            </textField>

            <textField pattern="C$ #,##0.00">
                <reportElement x="445" y="0" width="110" height="20"/>
                <textElement verticalAlignment="Middle"><font size="9" isBold="true"/></textElement>
                <textFieldExpression><![CDATA[$F{costoEnvio}]]></textFieldExpression>
            </textField>

            <line>
                <reportElement x="0" y="24" width="555" height="1" forecolor="#CCCCCC"/>
            </line>
        </band>
    </detail>

    <pageFooter>
        <band height="30">
            <textField>
                <reportElement x="435" y="0" width="80" height="20"/>
                <textElement textAlignment="Right"/>
                <textFieldExpression><![CDATA["Pág. " + $V{PAGE_NUMBER} + " de"]]></textFieldExpression>
            </textField>
            <textField evaluationTime="Report">
                <reportElement x="515" y="0" width="40" height="20"/>
                <textFieldExpression><![CDATA[" " + $V{PAGE_NUMBER}]]></textFieldExpression>
            </textField>
            <textField pattern="dd/MM/yyyy HH:mm">
                <reportElement x="10" y="0" width="150" height="20"/>
                <textFieldExpression><![CDATA[new java.util.Date()]]></textFieldExpression>
            </textField>
        </band>
    </pageFooter>
</jasperReport>